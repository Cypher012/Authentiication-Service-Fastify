{
  "name": "fastify-typescript-starter",
  "version": "4.2.0",
  "description": "Fastify TypeScript starter repository",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --watch src/app.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node src/app.ts",
    "typecheck": "tsc --noEmit",
    "drizzle:push": "drizzle-kit push",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.4",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "fastify-zod-openapi": "^5.5.0",
    "pg": "^8.17.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.8",
    "fastify-tsconfig": "^3.0.0",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
